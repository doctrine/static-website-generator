{
    "name": "doctrine/static-website-generator",
    "description": "Doctrine Static Website Generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan H. Wage",
            "email": "jonwage@gmail.com",
            "homepage": "https://jwage.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/collections": "^1.8",
        "doctrine/rst-parser": "^0.5.3",
        "erusev/parsedown": "^1.7",
        "symfony/filesystem": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/routing": "^6.0",
        "symfony/yaml": "^6.0",
        "twig/twig": "^3.5"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0",
        "doctrine/skeleton-mapper": "^2.0",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Doctrine\\StaticWebsiteGenerator\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctrine\\StaticWebsiteGenerator\\Tests\\": "tests/"
        }
    }
}
